// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("NEXT_DATABASE_URL")
}

model User {
  idusuario    Int     @id @default(autoincrement())
  email String
  nome  String?
  sobrenome  String?

  @@map("usuario")
}