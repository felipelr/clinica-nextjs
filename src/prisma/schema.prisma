// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("NEXT_DATABASE_URL")
}

model User {
  id   String @id @default(uuid())
  email String
  name  String
  domain  String
  password  String
  createdAt DateTime @default(now())

  @@map("user")
}